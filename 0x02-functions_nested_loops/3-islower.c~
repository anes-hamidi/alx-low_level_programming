#include "main.h"
/**
 * _islower - check if
 *
 * Description: check for lowercase character
 *
 * Return: Always 0 (success)
 */
/* function for chck character if islower or not*/
int _islower(int c)
{
/* check if character is lowecaser return 1 if is not lowercase return 0 */
if (c >= 'a'; &&c <= 'z')
{
/* return 1 (success) */
return (1);
}
else
{
/* return 0 */
return (0);
}
}
